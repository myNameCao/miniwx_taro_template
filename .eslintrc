{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }],
    "react/sort-comp": [1, {
      "order": [
        "config",
        "static-variables",
        "instance-variables",
        "lifecycle",
        "everything-else",
        "render"
      ]
    }]
  },
  "globals":{
    "wx":true,
    "amd": true
  },
    "parser": "babel-eslint"
  }
